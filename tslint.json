{
    "rulesDirectory": [
        "node_modules/vrsource-tslint-rules/rules",
        "node_modules/tslint-eslint-rules/dist/rules"
    ],
    "rules": {
        "file-header": [
            true,
            "MIT - Eli C Davis"
        ],
        "max-line-length": [
            true,
            100
        ],
        "no-console": [
            true,
            "log"
        ],
        "no-duplicate-imports": true,
        "no-duplicate-variable": true,
        "no-jasmine-focus": true,
        "no-var-keyword": true,
        "semicolon": [
            true
        ],
        "variable-name": [
            true,
            "ban-keywords"
        ],
        "no-inner-declarations": [
            true,
            "function"
        ]
    }
}